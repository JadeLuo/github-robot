{
  "name": "probot-triage",
  "version": "1.0.0",
  "description": "A Github Bot to triage issues and PRs",
  "author": "Olivier Combe <olivier.combe@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/ocombe/probot-triage.git",
  "scripts": {
    "prestart": "./node_modules/.bin/tsc",
    "start": "probot run ./dist/index.js",
    "start:watch": "nodemon --watch dist --watch .env ./node_modules/probot/bin/probot.js run ./dist/index.js",
    "start:dev": "concurrently \"tsc -w\" \"npm run start:watch\"",
    "test": "jest && standard"
  },
  "dependencies": {
    "@types/express": "^4.0.39",
    "@types/github": "^7.1.0",
    "@types/node": "^8.0.47",
    "probot": "^3.0.2",
    "probot-scheduler": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "concurrently": "^3.5.0",
    "jest": "^21.2.1",
    "localtunnel": "^1.8.2",
    "nodemon": "^1.12.1",
    "standard": "^10.0.3",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  },
  "engines": {
    "node": ">= 7.7.0",
    "npm": ">= 4.0.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  }
}
